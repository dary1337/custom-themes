{
     "manifest_version": 3,
     "name": "Custom Themes",
     "version": "1.1.0",
     "description": "Extension for applying custom themes by yuri",
     "permissions": ["scripting", "storage", "tabs", "webNavigation"],
     "host_permissions": ["https://*/*"],
     "action": {
          "default_popup": "index.html"
     },
     "background": {
          "service_worker": "src/worker.js"
     }
}
