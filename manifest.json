{
     "manifest_version": 3,
     "name": "Custom Themes",
     "version": "1.1.2",
     "description": "Extension for applying custom themes on websites",
     "permissions": ["scripting", "storage", "webNavigation"],
     "host_permissions": ["https://*/*"],
     "action": {
          "default_popup": "index.html"
     },
     "background": {
          "service_worker": "src/worker.js"
     }
}
